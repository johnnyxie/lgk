{
  "name": "lgk",
  "private": true,
  "version": "1.0.0",
  "description": "LGK League",
  "scripts": {
    "clean": "rimraf build",
    "start": "cd frontend; webpack-dev-server --progress --colors",
    "build": "cd frontend; npm run clean && NODE_ENV=production webpack --env.prod=true --progress --colors",
    "prod": "cd frontend; NODE_ENV=production webpack-dev-server --env.prod=true --progress --colors",
    "hapi": "env-cmd ./env.json node backend/server/src/server.js",
    "hapi-inspect": "env-cmd ./env.json node --inspect backend/server/src/server.js",
    "workers": "env-cmd ./env.json node backend/workers/src/pan-server-worker-manager.js",
    "nginx": "cd nginx;nginx -p `pwd` -c nginx.conf",
    "watch": "cd frontend;webpack --config webpack.config.dev.js --watch",
    "gen-nginx-conf": "env-cmd ./env.json node generate.js",
    "dev": "npm run start",
    "debug": "concurrently --kill-others \"npm run dev\" \"npm run workers\" \"npm run nginx\"",
    "main": "concurrently --kill-others \"npm run hapi\" \"npm run dev\" \"npm run workers\" \"npm run nginx\"",
    "debug-main": "concurrently --kill-others \"npm run hapi-inspect\" \"npm run dev\" \"npm run workers\" \"npm run nginx\"",
    "debug-backend": "concurrently --kill-others \"npm run workers\" \"npm run nginx\"",
    "backend": "concurrently --kill-others \"npm run hapi\" \"npm run workers\" \"npm run nginx\""
  },
  "author": "Jeffrey Xie",
  "license": "MIT",
